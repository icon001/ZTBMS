function TdmDBCreate.AlterTB_ACCESSDEVICE_Door2Type_Add: Boolean;
var
  stSql : string;
begin
  stSql := 'ALTER TABLE TB_ACCESSDEVICE ADD AC_DOOR2TYPE varchar(1) DEFAULT ''2'' NULL ';
  if G_nDBTYPE = MDB then
