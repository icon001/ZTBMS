  begin
    Close;
    Sql.Text := stSql;
    Try
      Open;
    Except
      Exit;
    End;

    if recordCount < 1 then Exit;
    stSql := 'ALTER TABLE TB_LOCATION DROP ' + FieldByName('index_name').AsString;
    dmDataBase.ProcessExecSQL(stSql);
  end;
end;

end.
