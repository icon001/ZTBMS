function TZeronEventDataBaseConfig.Table049VersionMake: Boolean;
begin
  //tb_employee 이름 사이즈 변경
  dmZeronEventDBCreate.AlterTB_EMPLOYEE_EmName_Change;
  //알람상태코드 사이즈 변경
  dmZeronEventDBCreate.AlterTB_ALARMSTATUSCODE_Code_Change;
  //알람 이벤트 상태코드 사이즈 변경
  dmZeronEventDBCreate.AlterTB_ALARMEVENT_Code_Change;
  //알람 이벤트 oper 사이즈 변경
  dmZeronEventDBCreate.AlterTB_ALARMEVENT_Oper_Change;
  //알람 이벤트 재정의 상태코드 생성
  dmZeronEventDBCreate.AlterTB_ALARMEVENT_STATECODE2_Add;
  //알람상태 그룹 테이블 생성
  dmZeronEventDBCreate.CreateTB_ALARMCODEGROUP;
  //알람 모드변경 제외 그룹 테이블 생성
  dmZeronEventDBCreate.CreateTB_ALARMMODENOTCARD;
  //알람 모드변경 제외 데이터 인서트
  dmZeronEventDBFunction.InsertIntoTB_ALARMMODENOTCARD_Value('001','***RESET**');
  dmZeronEventDBFunction.InsertIntoTB_ALARMMODENOTCARD_Value('002','.KTLinkUs.');
  dmZeronEventDBFunction.InsertIntoTB_ALARMMODENOTCARD_Value('003','ForceRelay');
  dmZeronEventDBFunction.InsertIntoTB_ALARMMODENOTCARD_Value('004','0000000000');
  dmZeronEventDBFunction.InsertIntoTB_ALARMMODENOTCARD_Value('005','RecoveryOK');
  dmZeronEventDBFunction.InsertIntoTB_ALARMSTATUSCODE_Value('ET','기타','0','0','0');
  result := dmZeronEventDBFunction.UpdateTB_CONFIG_Value('COMMON','TABLE_VER','49');

end;

