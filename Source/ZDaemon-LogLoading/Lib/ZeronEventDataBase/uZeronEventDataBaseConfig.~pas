  dmZeronEventDBFunction.InsertIntoTB_SONGHOFDCONFIG('SEMESTER','Y','150');

  dmZeronEventDBFunction.InsertIntoTB_SEMESTER('1','Y','0301','0614');
  dmZeronEventDBFunction.InsertIntoTB_SEMESTER('2','Y','0817','1129');
  dmZeronEventDBFunction.InsertIntoTB_SEMESTER('3','N','0101','0101');
  dmZeronEventDBFunction.InsertIntoTB_SEMESTER('4','N','1231','1231');

  result := dmZeronEventDBFunction.UpdateTB_CONFIG_Value('COMMON','TABLE_VER','176');
end;

