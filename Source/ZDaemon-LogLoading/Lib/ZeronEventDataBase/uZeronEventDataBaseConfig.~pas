function TZeronEventDataBaseConfig.SmallsetTable005VersionMake: Boolean;
begin
  dmZeronEventDBFunction.InsertIntoTB_CONFIG_All('SMALLSET','DBPOLLING','30');
  dmZeronEventDBCreate.CreateTB_ACCESSEVENT_IndexSMSENDSTATUS;
  dmZeronEventDBCreate.CreateTB_ALARMEVENT_IndexSMSENDSTATUS;

  result := dmZeronEventDBFunction.UpdateTB_CONFIG_Value('SMALLSET','TABLE_VER','5');
end;

function TZeronEventDataBaseConfig.SmallSetTableVersionCheck: Boolean;
var
  nTableVersion : integer;
begin
  result := False;
  nTableVersion := GetSmallSetTableVersion;
  if nTableVersion < 1 then SmallSetTable001VersionMake;
  if nTableVersion < 2 then SmallSetTable002VersionMake;
  if nTableVersion < 3 then SmallSetTable003VersionMake;
  if nTableVersion < 4 then SmallSetTable004VersionMake;
  if nTableVersion < 5 then SmallSetTable005VersionMake;

  result := True;
end;

