  dmZeronEventDBFunction.InsertIntoTB_ALARMSTATUSCODE_Value('EM','프로그램댐퍼','0','0','0');
  dmZeronEventDBFunction.InsertIntoTB_ALARMSTATUSCODE_Value('PF','TF단말확인','0','0','0');

  dmZeronEventDBFunction.InsertIntoTB_ALARMCODEGROUP_Value('Network','21');
  dmZeronEventDBFunction.InsertIntoTB_ALARMCODEGROUP_Value('Network','22');
  dmZeronEventDBFunction.InsertIntoTB_ALARMCODEGROUP_Value('Network','31');
