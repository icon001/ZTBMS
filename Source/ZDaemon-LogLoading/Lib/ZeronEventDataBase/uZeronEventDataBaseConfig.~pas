function TZeronEventDataBaseConfig.Table065VersionMake: Boolean;
var
  stSql : string;
begin
  G_nDaemonServerVersion := dmZeronEventDBFunction.GetDaemonVersion;
  if (G_nDaemonServerVersion > 0) and (G_nDaemonServerVersion < 4) then
  begin
    dmZeronEventDBFunction.InsertIntoTB_CONFIG_All('DAEMON','SCHASYNC','0');
  end else
  begin
    dmZeronEventDBFunction.InsertIntoTB_CONFIG_All('DAEMON','SCHASYNC','1');
  end;

  dmZeronEventDBFunction.InsertIntoTB_CONFIG_All('DAEMON','SCHDEVICE','0');

  result := dmZeronEventDBFunction.UpdateTB_CONFIG_Value('COMMON','TABLE_VER','65');

end;

