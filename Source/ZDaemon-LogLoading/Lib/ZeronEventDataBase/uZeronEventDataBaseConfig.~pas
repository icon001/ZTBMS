function TZeronEventDataBaseConfig.Table003VersionMake: Boolean;
var
  stPath : string;
begin
  stPath := 'C:\Program Files\ACServer\ACCESS.mdb';
  if G_nDBTYPE = POSTGRESQL then stPath := 'C:\\Program Files\\ACServer\\ACCESS.mdb';
  dmZeronEventDBFunction.InsertIntoTB_CONFIG_All('RELAY','fdmsUSE','FALSE');
  dmZeronEventDBFunction.InsertIntoTB_CONFIG_All('RELAY','fdmsPath',stPath);
  dmZeronEventDBFunction.InsertIntoTB_CONFIG_All('RELAY','fdmsPW','fdmsamho');
  dmZeronEventDBCreate.AlterTB_EMPLOYEE_FDMSID_Add;
  dmZeronEventDBCreate.AlterTB_EMPLOYEE_FDMSRELAY_Add;
  dmZeronEventDBCreate.AlterTB_DOOR_FDMSID_Add;
  result := dmZeronEventDBFunction.UpdateTB_CONFIG_Value('COMMON','TABLE_VER','3');
end;

