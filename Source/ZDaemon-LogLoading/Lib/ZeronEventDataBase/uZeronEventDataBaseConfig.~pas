function TZeronEventDataBaseConfig.Table182VersionMake: Boolean;
var
  stSql : string;
begin
  if G_nDBTYPE = MSSQL then dmZeronEventDBCreate.DropTB_COMPANY_PrimaryKey;
  dmZeronEventDBCreate.AlterTB_COMPANY_DEPARTCODE_Change;
  if G_nDBTYPE = MSSQL then dmZeronEventDBCreate.CreateTB_COMPANY_PrimaryKey;

  if G_nDBTYPE = MSSQL then dmZeronEventDBCreate.DropTB_LOCATION_PrimaryKey;
  dmZeronEventDBCreate.AlterTB_LOCATION_AREACODE_Change;
  if G_nDBTYPE = MSSQL then dmZeronEventDBCreate.CreateTB_LOCATION_PrimaryKey;

  dmZeronEventDBCreate.AlterTB_EMPLOYEE_DEPARTCODE_Change;
  dmZeronEventDBCreate.AlterTB_ACCESSDEVICE_AREACODE_Change;
  dmZeronEventDBCreate.AlterTB_ALARMDEVICE_AREACODE_Change;
  dmZeronEventDBCreate.AlterTB_ARMAREA_AREACODE_Change;
  dmZeronEventDBCreate.AlterTB_DOOR_AREACODE_Change;
  dmZeronEventDBCreate.AlterTB_FOOD_AREACODE_Change;
  dmZeronEventDBCreate.AlterTB_ZONEDEVICE_AREACODE_Change;

  dmZeronEventDBCreate.AlterTB_ACCESSDEVICE_ArmAreaUse_Add;
  stSql := ' Update TB_ACCESSDEVICE set AC_ARMAREAUSE = ''N'' ';
  dmZeronEventDataBase.ProcessExecSQL(stSql);
  dmZeronEventDBCreate.AlterTB_HOLIDAY_ACUSE_Add;
  dmZeronEventDBCreate.AlterTB_HOLIDAY_ATUSE_Add;
  stSql := ' Update TB_HOLIDAY set HO_ACUSE = ''1'',HO_ATUSE = ''1'' ';
  dmZeronEventDataBase.ProcessExecSQL(stSql);
  dmZeronEventDBCreate.AlterTB_COMPANY_VIEWSEQ_Add;
  dmZeronEventDBCreate.AlterTB_LOCATION_VIEWSEQ_Add;
  dmZeronEventDBCreate.AlterTB_POSI_VIEWSEQ_Add;


  if dmZeronEventDBFunction.CheckTB_COMPANY_Blank <> 1 then
  begin
    dmZeronEventDBFunction.InsertIntoTB_COMPANY_CompanyName('001','기본회사');
  end;

  result := dmZeronEventDBFunction.UpdateTB_CONFIG_Value('COMMON','TABLE_VER','182');
end;

function TZeronEventDataBaseConfig.Table183VersionMake: Boolean;
var
  stSql : string;
begin
  dmZeronEventDBCreate.CreateTB_CARDGROUPCODE;       //권한그룹 테이블 생성
  dmZeronEventDBFunction.InsertIntoTB_CARDGROUPCODE_All('0','Group','0','1','1'); //최초그룹코드는 조회 없이 초기 권한 부분때문에 임의로 생성함

  dmZeronEventDBCreate.CreateTB_DEVICECARDNOEMPLOYEE;
  dmZeronEventDBCreate.AlterTB_EMPLOYEE_CAGROUP_Add;

  result := dmZeronEventDBFunction.UpdateTB_CONFIG_Value('COMMON','TABLE_VER','183');
end;

