  dmZeronEventDBFunction.InsertIntoTB_CONFIG_All('CARD','CARDFIXED','FALSE','카드길이고정유무');
  dmZeronEventDBFunction.InsertIntoTB_CONFIG_All('CARD','FIXEDLEN','11','카드길이');
  dmZeronEventDBFunction.InsertIntoTB_CONFIG_All('CARD','FILLPOSI','0','채움문자위치 0:앞,1:뒤');
  dmZeronEventDBFunction.InsertIntoTB_CONFIG_All('CARD','FILLCHAR','0','채움문자');
  result := dmZeronEventDBFunction.UpdateTB_CONFIG_Value('COMMON','TABLE_VER','41');
end;

function TZeronEventDataBaseConfig.Table042VersionMake: Boolean;
var
  stCode1Name,stCode2Name : string;
begin
  dmZeronEventDBCreate.CreateTB_CARDTYPE;

  stCode1Name := '일반카드';
  stCode2Name := '기타카드';
  if CheckSamyuk then  //삼육대인경우
  begin
    stCode1Name := '일반카드';
    stCode2Name := '모바일카드';
  end else if CheckFdms then  //지문연동인 경우
  begin
    stCode1Name := '지문연동카드';
    stCode2Name := '일반카드';
  end;

  dmZeronEventDBFunction.InsertIntoTB_CARDTYPE_Value('1',stCode1Name);
  dmZeronEventDBFunction.InsertIntoTB_CARDTYPE_Value('2',stCode2Name);

  result := dmZeronEventDBFunction.UpdateTB_CONFIG_Value('COMMON','TABLE_VER','42');
end;

